{#
/**
 * @file
 * Default theme implementation for accordion item.
 *
 * Available variables:
 * - progress_id
 * - percent
 * - class
 * - icon
 * - content
 */
#}
<a role="button" class="btn {{ class }}" {% if link %} href="{{ link }}" {% endif %} {% if target %} target="{{ target }}" {% endif %}>
    <span class="btn_c_ic_a"><i class="{{ icon }}"></i></span>
    <span class="btn_c_t">{{ content }}</span>
    <span class="btn_c_ic_b"><i class="{{ icon }}"></i></span>
</a>


